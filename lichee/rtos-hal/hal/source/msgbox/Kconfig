menu "Message Box Devices"

config DRIVERS_MSGBOX
	bool "enable Message Box driver"
		default n

config DRIVERS_MSGBOX_SX
	bool "enable Message Box(SX) driver"
		depends on DRIVERS_MSGBOX
		default n

config DRIVERS_MSGBOX_AMP
	bool "enable Message Box(amp) driver"
		depends on DRIVERS_MSGBOX
		default n

config HAL_TEST_MSGBOX
	bool "enable msgbox hal APIs test command"
		depends on DRIVERS_MSGBOX
		default n
endmenu
